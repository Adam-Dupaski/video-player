<?xml version="1.0" encoding="utf-8"?>
<widget id="com.mendix.widget.custom.videoplayer.VideoPlayer" needsEntityContext="true" offlineCapable="true"
        pluginWidget="true"
        xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../xsd/widget.xsd">
    <name>Video Player</name>
    <description>Shows a video from Youtube or Vimeo</description>
    <icon>
        
    </icon>
    <properties>
        <property key="urlAttribute" type="attribute" required="false">
            <caption>Video URL</caption>
            <category>General</category>
            <description>The URL that contains the video</description>
            <attributeTypes>
                <attributeType name="String" />
            </attributeTypes>
        </property>
        <property key="urlValue" type="string" required="false">
            <caption>Static URL</caption>
            <category>General</category>
            <description>The  URL to show the video when no value is provided via the attribute</description>
        </property>
        <property key="posterAttribute" type="attribute" required="false">
            <caption>Poster Image</caption>
            <category>General</category>
            <description></description>
            <attributeTypes>
                <attributeType name="String" />
            </attributeTypes>
        </property>
        <property key="posterImage" type="string" required="false">
            <caption>Poster Image URL</caption>
            <category>General</category>
            <description></description>
        </property>
        <property key="autoStart" type="boolean" defaultValue="false">
            <caption>Auto start</caption>
            <category>Behavior</category>
            <description>Automatically start playing the video on page load.</description>
        </property>
        <property key="showControls" type="boolean" defaultValue="true">
            <caption>Show Controls</caption>
            <category>Behavior</category>
            <description>Whether to display the video controls (control bar, display icons and dock buttons). Available for youtube and external videos</description>
        </property>
        <property key="muted" type="boolean" defaultValue="true">
            <caption>Muted</caption>
            <category>Behavior</category>
            <description>Starts the video muted</description>
        </property>
        <property key="loop" type="boolean" defaultValue="true">
            <caption>Loop</caption>
            <category>Behavior</category>
            <description>Set the video to loop after it finishes</description>
        </property>
        <property key="widthUnit" type="enumeration" defaultValue="percentage">
            <caption>Width unit</caption>
            <category>Size</category>
            <description/>
            <enumerationValues>
                <enumerationValue key="percentage">Percentage</enumerationValue>
                <enumerationValue key="pixels">Pixels</enumerationValue>
            </enumerationValues>
        </property>
        <property key="width" type="integer" defaultValue="100">
            <caption>Width</caption>
            <category>Size</category>
            <description/>
        </property>
        <property key="heightUnit" type="enumeration" defaultValue="pixels">
            <caption>Height unit</caption>
            <category>Size</category>
            <description>'Percentage of width' is the aspect ratio, 'Pixels' is absolute. Warning: When using 'Percentage of parent' the parent container must have an absolute height, else nothing is displayed.</description>
            <enumerationValues>
                <enumerationValue key="percentageOfWidth">Percentage of width</enumerationValue>
                <enumerationValue key="pixels">Pixels</enumerationValue>
                <enumerationValue key="percentageOfParent">Percentage of parent</enumerationValue>
            </enumerationValues>
        </property>
        <property key="height" type="integer" defaultValue="500">
            <caption>Height</caption>
            <category>Size</category>
            <description/>
        </property>
    </properties>
</widget>
